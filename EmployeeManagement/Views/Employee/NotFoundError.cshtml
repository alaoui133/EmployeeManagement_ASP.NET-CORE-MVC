@model int
@{
    ViewBag.Title = "404 ERROR";
}

<div class="row justify-content-center">
    <div class="col-6 ">
        <div class="alert alert-dark" role="alert">
            <h4 class="text-danger"> 404 Not Found Error</h4>
            <p>Employee with <span class="text-info"> Id = @Model </span>Cannot be found.</p>
            <hr>
            <p class="mb-0"><a asp-action="Index" class="btn btn-dark"> Come Back to Employee List</a>.</p>
        </div>

    </div>
</div>

